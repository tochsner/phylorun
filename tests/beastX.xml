			<taxa idref="human-chimp"/>
		</mrca>
		<treeModel idref="treeModel"/>
	</tmrcaStatistic>
	<monophylyStatistic id="monophyly(human-chimp)">
		<mrca>
			<taxa idref="human-chimp"/>
		</mrca>
		<treeModel idref="treeModel"/>
	</monophylyStatistic>

	<!-- Generate a speciation likelihood for Yule or Birth Death                -->
	<speciationLikelihood id="speciation">
		<model>
			<yuleModel idref="yule"/>
		</model>
		<speciesTree>
			<treeModel idref="treeModel"/>
		</speciesTree>
		<calibration correction="exact">
			<point>
				<taxa idref="human-chimp"/>
				<normalDistributionModel>
					<mean>
						6.0
					</mean>
					<stdev>
						0.5
					</stdev>
				</normalDistributionModel>
			</point>
		</calibration>
	</speciationLikelihood>
	

	<!-- Generate a coalescent likelihood for the subtree defined by taxon set, human-chimp-->
	<coalescentLikelihood id="subtree.coalescent">
		<model>
			<constantSize idref="subtree.constant"/>
		</model>
		<populationTree>
			<treeModel idref="treeModel"/>
		</populationTree>
		<include>
			<taxa idref="human-chimp"/>
		</include>
	</coalescentLikelihood>
	

	<!-- The strict clock (Uniform rates across branches)                        -->
	<strictClockBranchRates id="branchRates">
		<rate>
			<parameter id="clock.rate" value="1.0" lower="0.0"/>
		</rate>
	</strictClockBranchRates>
	
	<rateStatistic id="meanRate" name="meanRate" mode="mean" internal="true" external="true">
		<treeModel idref="treeModel"/>
		<strictClockBranchRates idref="branchRates"/>
	</rateStatistic>
	

	<!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985)             -->
	<HKYModel id="noncoding.hky">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<alignment idref="alignment1"/>
				<frequencies>
					<parameter id="noncoding.frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<kappa>
			<parameter id="noncoding.kappa" value="2.0" lower="0.0"/>
		</kappa>
	</HKYModel>

	<!-- site model                                                              -->
	<siteModel id="noncoding.siteModel">
		<substitutionModel>
			<HKYModel idref="noncoding.hky"/>
		</substitutionModel>
		<relativeRate weight="4.380487804878049">
			<parameter id="noncoding.nu" value="0.25" lower="0.0" upper="1.0"/>
		</relativeRate>
		<gammaShape gammaCategories="4">
			<parameter id="noncoding.alpha" value="0.5" lower="0.0"/>
		</gammaShape>
	</siteModel>

	<!--                                                                         -->
	<statistic id="noncoding.mu" name="mu">
		<siteModel idref="noncoding.siteModel"/>
	</statistic>
	

	<!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985)             -->
	<HKYModel id="1stpos.hky">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<alignment idref="alignment2"/>
				<frequencies>
					<parameter id="1stpos.frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<kappa>
			<parameter id="1stpos.kappa" value="2.0" lower="0.0"/>
		</kappa>
	</HKYModel>

	<!-- site model                                                              -->
	<siteModel id="1stpos.siteModel">
		<substitutionModel>
			<HKYModel idref="1stpos.hky"/>
		</substitutionModel>
		<relativeRate weight="3.8874458874458875">
			<parameter id="1stpos.nu" value="0.25" lower="0.0" upper="1.0"/>
		</relativeRate>
		<gammaShape gammaCategories="4">
			<parameter id="1stpos.alpha" value="0.5" lower="0.0"/>
		</gammaShape>
	</siteModel>

	<!--                                                                         -->
	<statistic id="1stpos.mu" name="mu">
		<siteModel idref="1stpos.siteModel"/>
	</statistic>
	

	<!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985)             -->
	<HKYModel id="2ndpos.hky">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<alignment idref="alignment3"/>
				<frequencies>
					<parameter id="2ndpos.frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<kappa>
			<parameter id="2ndpos.kappa" value="2.0" lower="0.0"/>
		</kappa>
	</HKYModel>

	<!-- site model                                                              -->
	<siteModel id="2ndpos.siteModel">
		<substitutionModel>
			<HKYModel idref="2ndpos.hky"/>
		</substitutionModel>
		<relativeRate weight="3.8874458874458875">
			<parameter id="2ndpos.nu" value="0.25" lower="0.0" upper="1.0"/>
		</relativeRate>
		<gammaShape gammaCategories="4">
			<parameter id="2ndpos.alpha" value="0.5" lower="0.0"/>
		</gammaShape>
	</siteModel>

	<!--                                                                         -->
	<statistic id="2ndpos.mu" name="mu">
		<siteModel idref="2ndpos.siteModel"/>
	</statistic>
	

	<!-- The HKY substitution model (Hasegawa, Kishino & Yano, 1985)             -->
	<HKYModel id="3rdpos.hky">
		<frequencies>
			<frequencyModel dataType="nucleotide">
				<alignment idref="alignment4"/>
				<frequencies>
					<parameter id="3rdpos.frequencies" dimension="4"/>
				</frequencies>
			</frequencyModel>
		</frequencies>
		<kappa>
			<parameter id="3rdpos.kappa" value="2.0" lower="0.0"/>
		</kappa>
	</HKYModel>

	<!-- site model                                                              -->
	<siteModel id="3rdpos.siteModel">
		<substitutionModel>
			<HKYModel idref="3rdpos.hky"/>
		</substitutionModel>
		<relativeRate weight="3.8874458874458875">
			<parameter id="3rdpos.nu" value="0.25" lower="0.0" upper="1.0"/>
		</relativeRate>
		<gammaShape gammaCategories="4">
			<parameter id="3rdpos.alpha" value="0.5" lower="0.0"/>
		</gammaShape>
	</siteModel>

	<!--                                                                         -->
	<statistic id="3rdpos.mu" name="mu">
		<siteModel idref="3rdpos.siteModel"/>
	</statistic>
	

	<!-- Collecting together relative rates for partitions                       -->
	<compoundParameter id="allNus">
		<parameter idref="noncoding.nu"/>
		<parameter idref="1stpos.nu"/>
		<parameter idref="2ndpos.nu"/>
		<parameter idref="3rdpos.nu"/>
	</compoundParameter>
	

	<!-- Likelihood for tree given sequence data                                 -->
	<treeDataLikelihood id="treeLikelihood" useAmbiguities="false" usePreOrder="false">
		<partition>
			<patterns idref="noncoding.patterns"/>
			<siteModel idref="noncoding.siteModel"/>
		</partition>
		<partition>
			<patterns idref="1stpos.patterns"/>
			<siteModel idref="1stpos.siteModel"/>
		</partition>
		<partition>
			<patterns idref="2ndpos.patterns"/>
			<siteModel idref="2ndpos.siteModel"/>
		</partition>
		<partition>
			<patterns idref="3rdpos.patterns"/>
			<siteModel idref="3rdpos.siteModel"/>
		</partition>
		<treeModel idref="treeModel"/>
		<strictClockBranchRates idref="branchRates"/>
	</treeDataLikelihood>
	

	<!-- Define operators                                                        -->
	<operators id="operators" optimizationSchedule="log">
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="noncoding.kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="noncoding.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="1stpos.kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="1stpos.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="2ndpos.kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="2ndpos.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="3rdpos.kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1">
			<parameter idref="3rdpos.alpha"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="clock.rate"/>
		</scaleOperator>
		<upDownOperator scaleFactor="0.75" weight="3">
			<up>
				<parameter idref="treeModel.allInternalNodeHeights"/>
			</up>
			<down>
				<parameter idref="clock.rate"/>
			</down>
		</upDownOperator>
		<deltaExchange delta="0.01" weight="3">
			<parameter idref="allNus"/>
		</deltaExchange>
		<subtreeLeap size="1.0" weight="30">
			<treeModel idref="treeModel"/>
		</subtreeLeap>
		<fixedHeightSubtreePruneRegraft weight="3">
			<treeModel idref="treeModel"/>
		</fixedHeightSubtreePruneRegraft>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="yule.birthRate"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="subtree.constant.popSize"/>
		</scaleOperator>
	</operators>
	

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="1000000" autoOptimize="true">
		<joint id="joint">
			<prior id="prior">
				<booleanLikelihood>
					<monophylyStatistic idref="monophyly(human-chimp)"/>
				</booleanLikelihood>
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="noncoding.kappa"/>
				</logNormalPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="noncoding.alpha"/>
				</exponentialPrior>
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="1stpos.kappa"/>
				</logNormalPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="1stpos.alpha"/>
				</exponentialPrior>
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="2ndpos.kappa"/>
				</logNormalPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="2ndpos.alpha"/>
				</exponentialPrior>
				<logNormalPrior mu="1.0" sigma="1.25" offset="0.0">
					<parameter idref="3rdpos.kappa"/>
				</logNormalPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="3rdpos.alpha"/>
				</exponentialPrior>
				<dirichletPrior alpha="1.0" sumsTo="1.0">
					<parameter idref="allNus"/>
				</dirichletPrior>
				<ctmcScalePrior>
					<ctmcScale>
						<parameter idref="clock.rate"/>
					</ctmcScale>
					<treeModel idref="treeModel"/>
				</ctmcScalePrior>
				<gammaPrior shape="0.001" scale="1000.0" offset="0.0">
					<parameter idref="yule.birthRate"/>
				</gammaPrior>
				<gammaPrior shape="0.001" scale="1000.0" offset="0.0">
					<parameter idref="subtree.constant.popSize"/>
				</gammaPrior>
				<speciationLikelihood idref="speciation"/>
				
				<coalescentLikelihood idref="subtree.coalescent"/>
				
				
				<strictClockBranchRates idref="branchRates"/>
			</prior>
			<likelihood id="likelihood">
				<treeDataLikelihood idref="treeLikelihood"/>
			</likelihood>
		</joint>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="1000">
			<column label="Joint" dp="4" width="12">
				<joint idref="joint"/>
			</column>
			<column label="Prior" dp="4" width="12">
				<prior idref="prior"/>
			</column>
			<column label="Likelihood" dp="4" width="12">
				<likelihood idref="likelihood"/>
			</column>
			<column label="rootHeight" sf="6" width="12">
				<treeHeightStatistic idref="rootHeight"/>
			</column>
			<column label="clock.rate" sf="6" width="12">
				<parameter idref="clock.rate"/>
			</column>
		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="1000" fileName="beastx.log" overwrite="false">
			<joint idref="joint"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<treeHeightStatistic idref="rootHeight"/>
			<treeLengthStatistic idref="treeLength"/>
			<tmrcaStatistic idref="tmrca(human-chimp)"/>
			<parameter idref="yule.birthRate"/>
			<parameter idref="subtree.constant.popSize"/>
			<parameter idref="noncoding.kappa"/>
			<parameter idref="noncoding.alpha"/>
			<parameter idref="1stpos.kappa"/>
			<parameter idref="1stpos.alpha"/>
			<parameter idref="2ndpos.kappa"/>
			<parameter idref="2ndpos.alpha"/>
			<parameter idref="3rdpos.kappa"/>
			<parameter idref="3rdpos.alpha"/>
			<compoundParameter idref="allNus"/>
			<statistic idref="noncoding.mu"/>
			<statistic idref="1stpos.mu"/>
			<statistic idref="2ndpos.mu"/>
			<statistic idref="3rdpos.mu"/>
			<parameter idref="clock.rate"/>
			<rateStatistic idref="meanRate"/>
			<treeDataLikelihood idref="treeLikelihood"/>
			<strictClockBranchRates idref="branchRates"/>
			<speciationLikelihood idref="speciation"/>
			<coalescentLikelihood idref="subtree.coalescent"/>
			
		</log>

		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="1000" nexusFormat="true" fileName="beastx.trees" sortTranslationTable="true">
			<treeModel idref="treeModel"/>
			<trait name="rate" tag="rate">
				<strictClockBranchRates idref="branchRates"/>
			</trait>
			<joint idref="joint"/>
		</logTree>

		<!-- write state of Markov chain to checkpoint file                          -->
		<logCheckpoint id="checkpointFileLog" checkpointEvery="1000000" checkpointFinal="1000000" fileName="beastx.chkpt" overwrite="false"/>
	</mcmc>
	
	<report>
		<property name="timer">
			<mcmc idref="mcmc"/>
		</property>
	</report>
	
</beast>
